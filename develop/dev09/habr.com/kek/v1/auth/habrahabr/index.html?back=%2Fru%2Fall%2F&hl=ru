      <script>
  var googleClient;

  function signInCallback(authResult) {
    if (authResult['code']) {
      window.location.href = '/social/callback/google/?state='+authResult['state']+'&code='+authResult['code'];
    } else if (authResult['error']) {
      // Произошла ошибка.
      // Возможные коды ошибок:
      //   "access_denied" – пользователь отказался предоставить приложению доступ к данным
      //   "immediate_failed" – не удалось выполнить автоматический вход пользователя
      console.log('There was an error: ' + authResult['error']);
    }
  }

  function initGoogleClient() {
    googleClient = google.accounts.oauth2.initCodeClient({
      scope:"https://www.googleapis.com/auth/plus.login https://www.googleapis.com/auth/userinfo.email",
      ux_mode: 'popup',
      client_id:"472084966048-ut99mpprfh1n8ees0r26kr1183u1q4r0.apps.googleusercontent.com",
      state:"49dbe8a926b7b344c26330693aedd8bf",
      callback: signInCallback,
    });
  }

  function setGoogleReady() {
    var googleButton = document.querySelector('.google');
    if (googleButton) {
      googleButton.classList.remove('hidden');
    }
    initGoogleClient();
  };

  $('.google').on('click', function() {
    if (googleClient) {
      googleClient.requestCode();
    }
    return false; //чтобы не было отправки по ссылке
  });

</script>
<script async defer src="https://accounts.google.com/gsi/client" onload="setGoogleReady()" ></script>


