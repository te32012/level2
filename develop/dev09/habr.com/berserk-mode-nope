      <ul class="post-stats  js-user_" data-post-type="publish_corp_ru,c_otus,h_397,h_20726" id="infopanel_post_722572">
      <li class="post-stats__item">
          <div class="post-stats__result">

              <span class="post-stats__result-icon">
                <svg class="icon-svg_votes" width="10" height="16"><use xlink:href="https://habr.com/oldassets/6411ba2a/images/common-svg-sprite.svg#counter-rating" /></svg>
              </span>

            <span class="post-stats__result-counter  voting-wjt__counter_negative" title="Всего голосов 1: &uarr;0 и &darr;1">–1</span>
          </div>
      </li>
